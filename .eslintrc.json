{
  "extends": [
    "eslint:recommended",
    "plugin:@typescript-eslint/recommended",
    "next/core-web-vitals",
    "google",
    "plugin:storybook/recommended",
    "prettier"
  ],
  "rules": {
    "quotes": ["error", "single"],
    "comma-dangle": ["error", "always-multiline"],
    "require-jsdoc": "off"
  }
}
